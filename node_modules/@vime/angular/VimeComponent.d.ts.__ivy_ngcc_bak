import { PlayerProp } from '@vime/core';
import { ElementRef } from '@angular/core';
export declare abstract class VimeComponent {
    private readonly playerProps;
    private playerDispatch;
    private unbindPlayerContext;
    private playerCache;
    protected player?: HTMLVmPlayerElement;
    protected abstract ref: ElementRef;
    constructor(playerProps: PlayerProp[]);
    ngAfterViewInit(): Promise<void>;
    ngOnDestroy(): void;
}
